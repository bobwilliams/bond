.panel.panel-default
	.panel-heading: h3.panel-title Accounts
	.list-group
		a.list-group-item(ng-repeat='account in accounts', ng-click='editAccount(account)', href='')
			| {{ account.label }}
	.panel-footer
		strong Add Account:
		a.btn.btn-link(ng-repeat='(providerId, provider) in providers', href='')
			| {{ provider.name }}